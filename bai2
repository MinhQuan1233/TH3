package b3;

import java.text.NumberFormat;
import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.Date;
import java.util.Locale;

public class hanghoa {
	private int mahang;
    private String tenhang;
    private double dongia;
    private int soluongton;
    
    public hanghoa(int mahang, String tenhang, double dongia, int soluongton) {
    	this.mahang = mahang;
        this.tenhang = tenhang;
        this.dongia = dongia;
        this.soluongton = soluongton;
    }
    public int getmahang() {
    	return this.mahang;
    }
    public void setmahang(int mahang) {
    	this.mahang = mahang;
    }
    public String gettenhang() {
    	return this.tenhang;
    }
    public void settenhnag(String tenhang) {
    	this.tenhang = tenhang;
    }
    public double getdongiag() {
    	return this.dongia;
    }
    public void setdongia(double dongia) {
    	 this.dongia = dongia;
    }
    public int getsoluongton() {
    	return this.soluongton;
    }
    public void setsoluongton(int soluongton) {
    	this.soluongton = soluongton;    
    }
    public class thucpham
    {
    	private Date nsx, hsd;
        private String nhsx;
        
        public thucpham(Date nsx, Date hsd,  String nhsx) {
        	this.hsd = hsd;
            this.nsx = nsx;
            this.nhsx = nhsx;
        }
        public Date getnsx() {
        	return this.nsx;
        }
        public void setnsx(Date nsx) {
            this.nsx = nsx;
        }
        public Date gethsd() {
        	return this.hsd;
        }
        public void sethsd(Date hsd) {
            this.hsd = hsd;
        }
        public String getnhsx() {
        	return this.nhsx;
        }
        public void setnhsx(String nhsx) {
        	this.nhsx = nhsx;    
        }
    }
}
    
   
